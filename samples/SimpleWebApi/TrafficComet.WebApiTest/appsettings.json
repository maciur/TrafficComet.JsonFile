{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    }
  },
  "TrafficComet": {
    "Generator": {
      "ClientId": {
        "CookieName": "calc_traffic",
        "HeaderName": "Traffic-Client-Id"
      },
      "TraceId": {
        "HeaderName": "Traffic-Trace-Id"
      }
    },
    "Writers": {
      "JsonFile": {
        "Folder": "C:\\traffic-comet\\traffic-logs"
      }
    },
    "Middleware": {
      "Root": {
        "ApplicationId": "WebApiTest"
      }
    }
  }
}